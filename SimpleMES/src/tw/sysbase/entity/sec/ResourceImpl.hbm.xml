<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

	<class name="tw.sysbase.entity.sec.ResourceImpl" table="t_sec_resource">

		<id name="id" column="F_PID" length="32" type="string"
			unsaved-value="">
			<generator class="uuid.hex" />
		</id>
		<property name="targetId" column="F_TARGET_ID" length="32"
			type="string" />
		<property name="type" column="F_TYPE_ID" length="32" type="string" />

	</class>

</hibernate-mapping>