<script src="/platform/support/js/jquery.min.js"></script>
<script type="text/javascript">
$(function () {
	 
    setTimeout(ChangeTime, 1000);

  });

  function ChangeTime() {

    var time;

    time = $("#time").text();    

    time = parseInt(time);

    time--;

    if (time <= 0) {

      window.location.href = "/platform/Admin/login.html";

    } else {

      $("#time").text(time);

      setTimeout(ChangeTime, 1000);

    }

  }
</script>