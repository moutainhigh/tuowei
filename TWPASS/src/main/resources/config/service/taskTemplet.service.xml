<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	default-lazy-init="true">
	<!--数据日志管理-->
	<bean id="myLogDal" class="com.talkweb.xwzcxt.dal.MyLogDal" />
	<bean id="myLogServiceImpl" class="com.talkweb.xwzcxt.service.impl.MyLogServiceImpl" />
	<!-- 岗位活动模版 -->
	<bean id="sTempletDal" class="com.talkweb.xwzcxt.dal.STempletDal" />
	<bean id="sTempletService" class="com.talkweb.xwzcxt.service.impl.STempletServiceImpl" />

	<!-- 岗位活动模板与二维码关联 -->
	<bean id="tSdTempletScancodeDal" class="com.talkweb.xwzcxt.dal.TSdTempletScancodeDal" />

	<!-- 岗位活动 -->
	<bean id="tsdActnodeDal" class="com.talkweb.xwzcxt.dal.TSdActnodeDal" />
	<bean id="tsdActnodeService" class="com.talkweb.xwzcxt.service.impl.TSdActnodeServiceImpl" />

	<!-- 岗位活动项 -->
	<bean id="tsdActnodeItemDal" class="com.talkweb.xwzcxt.dal.TSdActnodeItemDal" />
	<bean id="tsdActnodeItemService" class="com.talkweb.xwzcxt.service.impl.TSdActnodeItemServiceImpl" />

	<!-- 多媒体 -->
	<bean id="tSdMediastdDal" class="com.talkweb.xwzcxt.dal.TSdMediastdDal" />

	<!-- 岗位规则 -->
	<bean id="rulePositionDal" class="com.talkweb.xwzcxt.dal.RulePositionDal" />
	<bean id="rulePositionService" class="com.talkweb.xwzcxt.service.impl.RulePositionServiceImpl" />

	<!-- 时间规则 -->
	<bean id="ruleTimeDal" class="com.talkweb.xwzcxt.dal.RuleTimeDal" />
	<bean id="ruleTimeService" class="com.talkweb.xwzcxt.service.impl.RuleTimeServiceImpl" />
</beans>