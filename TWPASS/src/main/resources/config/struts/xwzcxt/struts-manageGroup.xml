<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="struts-jsonManageGroup" extends="json-default">
		<action name="ManageGroup_*" class="com.talkweb.xwzcxt.action.ManageGroupAction"
			method="{1}">
			<param name="aliases">#{'page' : 'pagination.currPage','rows' :
				'pagination.size','pageSize' : 'pagination.size'}</param>
			<result name="success" type="json">
				<param name="root">data</param>
			</result>
		</action>
	</package>
</struts>
