<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="authorityManage" extends="twdpe-common-default">
		<action name="getPrivilegeResourceModesAction" class="com.talkweb.xwzcxt.action.twdpe.AuthorityManageAction"
			method="getPrivilegeResourceModes">
			<result name="success" type="json">
				<param name="root">data</param>
				<param name="excludeNullProperties">true</param>
			</result>
		</action>
		<action name="getPrivilegeDataModesAction" class="com.talkweb.xwzcxt.action.twdpe.AuthorityManageAction"
			method="getPrivilegeDataModes">
			<result name="success" type="json">
				<param name="root">data</param>
				<param name="excludeNullProperties">true</param>
			</result>
		</action>
	</package>
</struts>