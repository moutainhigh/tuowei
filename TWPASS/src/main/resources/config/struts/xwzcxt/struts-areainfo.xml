<struts>
	<package name="struts-jsonareainfo" extends="twdpe-common-default">
	    
	<action name="areaInfo_init"  class="com.talkweb.xwzcxt.action.AreaInfoAction" method="initFromControl">
		<result name="success" type="json">
			<param name="root">data</param>
		</result>
		<result name="error">/error/error.jsp</result>
	</action>

	<action name="areaInfo_areaType" class="com.talkweb.xwzcxt.action.AreaInfoAction"
		method="getAreaTypeById">
		<result name="success" type="json">
			<param name="root">data</param>
		</result>
		<result name="error">/error/error.jsp</result>
	</action>
	
	<action name="areaInfo_updateArea" class="com.talkweb.xwzcxt.action.AreaInfoAction"
		method="updateArea">
		<result name="success" type="json">
			<param name="root">data</param>
		</result>
		<result name="error">/error/error.jsp</result>
	</action>
	<action name="areaInfo_updateScanCode" class="com.talkweb.xwzcxt.action.AreaInfoAction"
		method="updateScanCode">
		<result name="success" type="json">
			<param name="root">data</param>
		</result>
		<result name="error">/error/error.jsp</result>
	</action>
	
		<action name="areaInfo_addArea" class="com.talkweb.xwzcxt.action.AreaInfoAction"
		method="addArea">
		<result name="success" type="json">
			<param name="root">data</param>
		</result>
		<result name="error">/error/error.jsp</result>
	  </action>
	 
	 <action name="areaInfo_deleteArea" class="com.talkweb.xwzcxt.action.AreaInfoAction"
		method="deleteArea">
		<result name="success" type="json">
			<param name="root">data</param>
		</result>
		<result name="error">/error/error.jsp</result>
	 </action>
	 
	 <action name="areaInfo_PrintCode" class="com.talkweb.xwzcxt.action.AreaInfoAction"
		method="printCode">
		<result name="success" type="json">
			<param name="root">data</param>
		</result>
		<result name="error">/error/error.jsp</result>
	</action>
	
	 <action name="areaInfo_getCurrentPrintCount" class="com.talkweb.xwzcxt.action.AreaInfoAction"
		method="getCurrentPrintCount">
		<result name="success" type="json">
			<param name="root">data</param>
		</result>
		<result name="error">/error/error.jsp</result>
	</action>
	


    </package>

</struts>

